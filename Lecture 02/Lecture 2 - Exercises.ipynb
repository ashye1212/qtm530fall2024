    "# Write your own code\n",
    "\n",
    "r = 0.04\n",
    "\n",
    "# Define arrays A and T\n",
    "A = np.array([20, 30, 15])\n",
    "T = np.array([0, 1, 2])\n",
    "\n",
    "# Compute M using element-by-element operations\n",
    "M = A / (1 + r) ** T\n",
    "\n",
    "# Compute the sum of the values in M\n",
    "M_sum = M.sum()\n",
    "\n",
    "# Output the array M and its sum\n",
    "M, M_sum\n",
    "\n"
